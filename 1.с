#include <stdio.h>

#define size 100

void main() {
	int a[size];
	int n,index=0,b=0;
	int emul=1,sum=0,max=0;
	

	scanf("%d", &n);
	if (n<=0 || n > size)
	{
		printf("Error! Max 100");
	}
	else
	{
		for (int i = 0; i < n; i++)
		{
			printf("[%d%s", b++,"]=");
			scanf("%d", &a[i]);
		}
			for (int i = 0; i < n; i++)
			{
				if (a[i] >= max) 
				{
					max = a[i];
					index = i;
				}
			}
			if (index == n - 1)
			{
				printf("no numbers");
			}
			if (index < n - 1)
			{
				for (int i = index + 1; i < n; i++)
				{
					if (a[i] > 0)
					{
						sum = sum + a[i]*2;
					}
					else if (a[i] < 0)
					{
						emul = emul * a[i];
					}
				}
				
			}
	}
	if (sum > 0)
	{
		printf("Summa=%d", sum);
	}
	if (emul < 1)
	{
		emul = emul * 2;
		printf("composition=%d", emul);
	}
return 0;
}
